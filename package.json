    "@types/classnames": "2.3.1",
    "@types/gtag.js": "^0.0.8",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "autoprefixer": "10.4.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "8.3.0",
